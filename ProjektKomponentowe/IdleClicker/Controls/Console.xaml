<UserControl x:Class="IdleClicker.Console"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IdleClicker"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="250" Height="150">
    <Grid Width="250" Height="150">
        <TextBox x:Name="textBox" Height="23" Margin="0" TextWrapping="Wrap" VerticalAlignment="Bottom" Background="#FFE4B57B" Foreground="#FF492412" FontFamily="/IdleClicker;component/Resources/Fonts/#Eczar"/>
        <ListBox x:Name="listBox" Margin="0" Width="250" Height="127" VerticalAlignment="Top" Background="#FF8A4A27" Foreground="#FF492412" FontFamily="/IdleClicker;component/Resources/Fonts/#Eczar"/>
    </Grid>
</UserControl>
